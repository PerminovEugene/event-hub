  "description": "My webpack project",
  "main": "./built/index.js",
  "engines": {
    "node": "10.16.3",
    "npm": "6.9.0"
  },
  "scripts": {
    "dev": "npm-run-all --parallel nodemon-dev-server bsdw bcdw test:watch",
    "start": "webpack-dev-server --mode development --open --hot",
    "build": "webpack --mode production",
    "build:server:dev": "webpack --config=./src/framework/build/webpack.dev.server.config.js",
    "build:server:dev:watch": "webpack --config=./src/framework/build/webpack.dev.server.config.js --watch",
    "bsd": "npm run build:server:dev",
    "bsdw": "npm run build:server:dev:watch",
    "build:client:dev": "webpack --config=./src/framework/build/webpack.dev.client.config.js",
    "build:client:dev:watch": "webpack --config=./src/framework/build/webpack.dev.client.config.js --watch",
    "bcd": "npm run build:client:dev",
    "bcdw": "npm run build:client:dev:watch",
    "start-dev-server": "node builded-server/build.js",
    "nodemon-dev-server": "./node_modules/nodemon/bin/nodemon.js --exec 'npm run start-dev-server'",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "author": "Eugene Perminov",
