{
  "name": "@event-hub/api-server",
  "version": "0.1.2",
  "description": "",
  "author": "Eugene Perminov",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "build:prod": "nest build --path=./tsconfig.prod.json",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./jest-e2e.json --runInBand --detectOpenHandles",
    "test:e2e:watch": "jest --config ./jest-e2e.json --runInBand --detectOpenHandles --watch",
    "test:integration": "jest --config ./jest-integration.json --runInBand --detectOpenHandles",
    "test:integration:watch": "jest --config ./jest-integration.json --runInBand --detectOpenHandles --watch",
    "test:all": "jest --config ./jest-all.json --runInBand --detectOpenHandles",
    "clean": "rm -rf ./dist",
    "generate:gql:defenitions": "ts-node ./src/scripts/generate-graphql-definitions.ts",
    "generate:fixtures": "ts-node src/database/fixtures/executor.ts",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@event-hub/shared": "^0.1.1",
    "@hapi/joi": "^17.1.0",
    "@nestjs/common": "^6.7.2",
    "@nestjs/core": "^6.10.13",
    "@nestjs/graphql": "^6.5.3",
    "@nestjs/passport": "^6.1.0",
    "@nestjs/platform-express": "^6.7.2",
    "apollo-server-express": "^2.9.7",
    "bcrypt": "^3.0.6",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.11.0",
    "cookie-parser": "^1.4.4",
    "dataloader": "^2.0.0",
    "dotenv": "^8.2.0",
    "express-session": "^1.17.0",
    "graphql": "^14.5.8",
    "graphql-tools": "^4.0.5",
    "js-yaml": "^3.13.1",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "pg": "^7.14.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.0",
    "ts": "^0.2.2",
    "typeorm": "^0.2.21",
    "@types/cookie-parser": "^1.4.2",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.1",
    "@types/express-session": "^1.15.15",
    "@nestjs/cli": "^6.12.9",
    "@nestjs/schematics": "^6.7.0",
    "@types/js-yaml": "^3.12.1",
    "@types/node": "^12.7.5",
    "@types/passport-local": "^1.0.33",
    "ts-node": "^8.4.1"
  },
  "devDependencies": {
    "@nestjs/testing": "^6.7.1",
    "@types/faker": "^4.1.9",
    "@types/jest": "^24.9.0",
    "@types/supertest": "^2.0.8",
    "faker": "^4.1.0",
    "jest": "^24.9.0",
    "prettier": "^1.18.2",
    "supertest": "^4.0.2",
    "ts-jest": "^24.1.0",
    "ts-loader": "^6.1.1",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^5.20.0",
    "typescript": "^3.7.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "./coverage",
    "testEnvironment": "node"
  },
  "private": true
}
